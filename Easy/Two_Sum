#Hashmap Solution
Time Complexity: O(N)
Space Complexity: O(N)

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
     unordered_map<int,int>cache;
     for(int x = 0; x<nums.size(); x++){
         int diff = target-nums[x];
         if(cache.find(diff)!=cache.end()) return {cache[diff],x};
         cache[nums[x]] = x;
     }
        return {};
    }
};
